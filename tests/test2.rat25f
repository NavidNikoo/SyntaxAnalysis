"---- Program to find the area of a circle ----"
function square ( r real )
real result ;
{
    result = r * r ;
    return result ;
}
function main ( )
real r , area ;
{
    "Ask user for radius"
    put ( r ) ;
    get ( r ) ;

    area = 3.1416 * square ( r ) ;
    "Display the computed area"
    put ( area ) ;

    return ;
}